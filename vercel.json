{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/dist/static/$1"
    },
    {
      "src": "/(.*)\\.br",
      "dest": "/dist/$1.br",
      "headers": {
        "Content-Encoding": "br"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
